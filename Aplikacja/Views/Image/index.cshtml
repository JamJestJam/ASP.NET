@model ICollection<Aplikacja.Models.Image>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<table>
    <tr>
        <td>ID</td>
        <td>Pokaż więcej</td>
    </tr>
    @foreach (var image in Model)
    {
        <tr>
            <td>@image.ImageID </td>
            <td><a class="btn btn-primary" asp-controller="Image" asp-action="Show" asp-route-id="@image.ImageID">Pokaz</a></td>
        </tr>
    }
</table>